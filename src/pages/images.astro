---


import Layout from "@/layouts/Layout.astro";


import postimages from "@/data/postimages"
--- 


<Layout>
    <div>
   
     
       <section class="">
               
     
            <div class="grid grid-rows-8 grid-cols-4 gap-4">
              {postimages.slice(0,100).map((image) => (
                <div><a href={image.path} style=""><img src={image.path} alt="" /></a></div>
              ))}
            </div>    
         
        </section>
      
    </div>
</Layout>
